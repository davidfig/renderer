  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidfig/renderer/issues"
  },
  "homepage": "https://github.com/davidfig/renderer#readme",
  "devDependencies": {
    "browserify": "^13.1.0"
  },
  "dependencies": {
    "highlight.js": "^9.8.0",
    "penner": "^0.1.3",
    "pixi.js": "^4.2.2",
    "yy-animate": "^1.3.0",
    "yy-debug": "^1.0.8",
    "yy-update": "^1.0.7"
  }
}
